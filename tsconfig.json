{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react",
    "noEmit": false,
    "isolatedModules": true,
    "noImplicitAny": true
  },
  "exclude": ["node_modules", "dist"]
}
